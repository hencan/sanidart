<div class="container my-4">
  <div class="card">
    <div class="card-header">Perfis</div>
    <div class="card-body">
      <h5 class="card-title">Perfil ID: <%= @profile.id %></h5>
      <div class="card-text">
        <p>
          <strong>Imagem:</strong>
          <br>
          <% if @profile.image.nil? %>
            <img style='width:300px' class="img-thumbnail" src="data:image/jpeg;base64,">
          <% else %>
            <%= image_tag(@profile.image, style:'width:300px', class: "img-thumbnail") %>
          <% end %>
        </p>
        <p>
          <strong>Nome:</strong>
          <%= @profile.name %>
        </p>
        <p>
          <strong>Habilidades:</strong>
        </p>
        <div id="skills_area" class="ql-editor"><%= @profile.skills %></div>
        <br>
        <p>
          <strong>Whatsapp:</strong>
          <%= @profile.whatsapp %>
        </p>
        <p>
          <strong>Telegram:</strong>
          <%= @profile.telegram %>
        </p>
        <p>
          <strong>Email:</strong>
          <%= @profile.email %>
        </p>
        <p>
          <strong>Facebook:</strong>
          <%= @profile.facebook %>
        </p>
        <p>
          <strong>Instagram:</strong>
          <%= @profile.instagram %>
        </p>
        <p>
          <strong>Linkedin:</strong>
          <%= @profile.linkedin %>
        </p>
        <p>
          <strong>Twitter:</strong>
          <%= @profile.twitter %>
        </p>
        <p>
          <strong>Youtube:</strong>
          <%= @profile.youtube %>
        </p>
        <p>
          <% if @profile.main %>
            <i class="bi bi-check-square me-2"></i><strong>Principal</strong>
          <% else %>
            <i class="bi bi-square me-2"></i><strong>Principal</strong>
          <% end %>
        </p>
      </div>
      <div>
        <%= link_to 'Editar', edit_profile_path(@profile), class:"btn btn-warning me-2" %>
        <%= link_to 'Voltar', profiles_path, class:"btn btn-outline-dark"  %>
      </div>
    </div>
    <div class="card-footer text-muted"></div>
  </div>
</div>
<script>
  document.getElementById("skills_area").innerHTML = document.getElementById("skills_area").innerHTML.replace(/&lt;/g, "<").replace(/&gt;/g, ">")
</script>